{
  "name": "obsidian-stoic-theme",
  "version": "0.0.1",
  "description": "A minimalist, high-contrast theme inspired by Stoicism",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build/",
    "watch:stoic": "sass --watch --no-source-map src/Stoic.scss stoic-theme-design-vault/.obsidian/themes/Stoic.css",
    "watch:stoic-publish": "sass --watch --no-source-map src/StoicPublish.scss stoic-theme-design-vault/publish.css",
    "build:stoic": "sass --no-source-map src/Stoic.scss build/Stoic.css",
    "build:stoic-publish": "sass --no-source-map src/StoicPublish.scss build/publish.css",
    "build": "npm run build:stoic && npm run build:stoic-publish",
    "dist": "sh dist.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikbrunner/obsidian-stoic-theme.git"
  },
  "keywords": [
    "obsidian",
    "theme"
  ],
  "author": "nibru",
  "license": "UNI",
  "bugs": {
    "url": "https://github.com/nikbrunner/obsidian-stoic-theme/issues"
  },
  "homepage": "https://github.com/nikbrunner/obsidian-stoic-theme#readme",
  "dependencies": {
    "prettier": "^2.2.1",
    "sass": "^1.32.5"
  }
}
